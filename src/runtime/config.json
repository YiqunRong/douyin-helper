{
  "selectors": {
    "feed:slides": ".swiper-slide",
    "feed:slide": ".swiper-slide-active",
    "feed:slide:video": "video",
    "feed:slide:videoWrapper": ".playerContainer",
    "feed:slide:showCommentBtn": ".NRiH5zYV:nth-child(3) > div",
    "feed:slide:desc": ".video-info-detail > .title",
    "feed:slide:desc:hash": "span",
    "feed:slide:playBtn:playing": ".xgplayer-play[data-state=\"play\"]",
    "feed:slide:switch:prev": ".xgplayer-playswitch-prev",
    "feed:slide:switch:next": ".xgplayer-playswitch-next",
    "feed:slide:account": ".video-info-detail .account",
    "feed:slide:accountBadge": ".video-info-detail .account-badage",
    "video": "video",
    "darkTheme": "html[dark=\"true\"]"
  },
  "paths": {
    "feeds": [
      "/",
      "/follow"
    ],
    "remapShortcutsIn": [
      "/",
      "/follow",
      "/video/*"
    ]
  },
  "apis": {
    "feed": {
      "path": "/aweme/v1/web/tab/feed/"
    },
    "followFeed": {
      "path": "/aweme/v1/web/follow/feed/"
    }
  },
  "search": {
    "rememberableOptionKeys": ["type"]
  }
}
